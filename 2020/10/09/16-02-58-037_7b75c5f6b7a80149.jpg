<script>
function _slugify( str ) {
    return str.toString().toLowerCase()
        .replace(/\s+/g, '-')
        .replace(/[^\w\-]+/g, '')
        .replace(/\-\-+/g, '-');
}

function _tocJump(where) {

    var _where = where.split('#')[1];
    var _target_y = jQuery('#' + _where).offset().top;
    jQuery('html,body').animate({ scrollTop: _target_y - 130 }, 350);
    window.location.hash = '#' + _where;
}

function _toc() {

    var _article = document.querySelector("article#post-50927 > main > section");
    var _toc = '<ul class="toc">';
    var _hs = [].slice.call( _article.querySelectorAll('h2, h3:not([class])') );
        _hs.forEach(function(el, i) {

            var _class = 'header-' + el.tagName.toLowerCase();
            var _txt = el.innerText;
            var _id = 'toc-' + _slugify(_txt) + '-' + i;
                el.id = _id; // Set anchor
            if (!!_txt) { // Set menu item
                _toc += '\n';
                _toc += '<li class="' + _class + '"><a href="#' + _id + '" title="' + _txt + '" onclick="_tocJump(this.href); return false;">' + _txt + '</a></li>';
            }
        });

    // Close TOC
    _toc += '\n';
    _toc += '</ul>';
    _toc = (!_hs.length) ? '' : _toc;

    // Inject TOC
    var toc_nav = document.querySelector('nav#toc-nav');
    var is_multipost = !!toc_nav.querySelector('#serial-posts-wrapper')
    if ( is_multipost ) {

        var _current = toc_nav.querySelector('.current-inactive');
            _current.innerHTML += _toc

        var _toc_items = toc_nav.querySelectorAll('li.serial-posts-list-item');
            _toc_items = Array.from(_toc_items);
            _toc_items.forEach(function(el, i) {
                el.innerHTML = 'Part '+ (i+1) +': ' + el.innerHTML;
            });

        var _ultoc_open = true;
        _current.addEventListener('click', function(e) {
            if ( _ultoc_open == true ) {
                this.querySelector('ul.toc').style.display = 'none';
                _ultoc_open = false;
            } else {
                this.querySelector('ul.toc').style.display = 'block';
                _ultoc_open = true;
            }
        });
        _current.querySelector('ul').addEventListener('click', function(e) {
            e.stopPropagation();
        });

    } else {

        var _toc_title = '<h3 class="serial-posts-heading">Table of Contents</h3>';
        toc_nav.innerHTML += (_toc.length > 0) ? _toc_title : '';
        toc_nav.innerHTML += _toc;
    }
}

function _tocOpenListener() {

    [].forEach.call( document.querySelectorAll('#toc-nav-status, #toc-nav ul li a'), function(el, i) {
        el.addEventListener('click', function() {
            document.querySelector('#toc-nav').classList.toggle('open');
        });
    });
}

function _highlighter() {

    var _pre = document.querySelectorAll('pre');
        _pre = Array.from(_pre);
        _pre.forEach(function(el) {

            var _current = el.innerHTML;
            var _code = document.createElement('code');
                _code.innerHTML = _current;

            el.innerHTML = '';
            el.appendChild(_code);

            hljs.highlightBlock(_code);
        });
}

function _scrollBar() {

    var _scrolled = Math.round((
        (document.body.scrollTop || document.documentElement.scrollTop) /
        ((document.documentElement.scrollHeight - document.documentElement.clientHeight) - document.querySelector('.footer-area').offsetHeight)) * 100);
        _scrolled = Math.min(100, _scrolled)

    document.querySelector('#scroll-status-percent').style.width = _scrolled + '%';
}

function _twitterButton() {

    // Positioning
    var _twitter_button_container = document.querySelector('#twitter-footer-button');
    var _twitter_button_target = document.createElement('div');
        _twitter_button_target.innerHTML = _twitter_button_container.innerHTML;
    var _twitter_button_new = document.querySelector('#the-content').insertBefore(_twitter_button_target, document.querySelector('.sharedaddy'));
        _twitter_button_container.innerHTML = '';

    var _twitter_button = document.getElementById('continue-conversation-twitter');
        _twitter_button.onclick = function(e) {
            e.preventDefault();
            var _master = document.querySelector('li.share-twitter a');
                _master.click();
        }
}

function _sharerPosition() {

    document.querySelector('.aside-troubleshooting').innerHTML = document.querySelector('.sharedaddy').innerHTML;
}

function _checkActiveItem(sections) {

    var _viewportHeight = window.innerHeight || document.documentElement.clientHeight;
    var _output = [];
    for (var i = 0; i < sections.length; i++) {
        var _s = document.querySelector('#the-content #' + sections[i]).getBoundingClientRect();
        if (_s.y < _viewportHeight) {
            _output.push({'i':i, 'h':_s.y});
        }
    }

    var _el = false;
    if ( _output.length == 1 ) { _el = _output[0].i; }
    if ( _output.length >= 2 ) {
        var _last = _output[_output.length-1];
        var _penu = _output[_output.length-2];
        _el = ( _last.h < 200 ) ? _last.i : _penu.i
    }

    // Set if applicable
    if ( _el !== false && (_el || _el === 0) ) { _setActiveItem(_el); } else { _removeActiveItem() }
}

function _removeActiveItem() {
    var els = document.querySelectorAll('nav#toc-nav ul.toc a');
    [].forEach.call( els, function(el) {
        el.classList.remove('active');
    })

}
function _setActiveItem(active) {
    var els = document.querySelectorAll('nav#toc-nav ul.toc a');
    [].forEach.call( els, function(el, i) {
        if ( i == active ) {
            el.classList.add('active');
        } else {
            el.classList.remove('active');
        }
    })
}

function _getSections() {

    return Array.from(document.querySelectorAll('nav#toc-nav ul.toc a')).map(function(el) {
        return el.href.split('#')[1];
    });
}

function _fiColor() {

    function _rgbToHex(rgb) {
        function cHex(c) {
            let _hex = c.toString(16);
            return _hex.length == 1 ? "0" + _hex : _hex;
        }
        return "#" + cHex(rgb.r) + cHex(rgb.g) + cHex(rgb.b);
    }

    let _canvas = document.createElement('canvas');
    let _context = _canvas.getContext && _canvas.getContext('2d');
    let _rgb = { r:0, g:0, b:0 };

    let _fi = document.querySelector('#the-featured-image');
    let _height = _canvas.height = _fi.naturalHeight || _fi.offsetHeight || _fi.height;
    let _width = _canvas.width = _fi.naturalWidth || _fi.offsetWidth || _fi.width;

    let _xhr = new XMLHttpRequest();

        _xhr.open('GET', _fi.src);
        // _xhr.open('GET', 'https://cors-anywhere.herokuapp.com/' . _fi.src);
        _xhr.setRequestHeader('X-Requested-With', 'XMLHttpRequest');
        _xhr.responseType = "blob";
        _xhr.onload = function() {

            let _urlCreator = window.URL || window.webkitURL;
            let _imgUrl = _urlCreator.createObjectURL( _xhr.response );

            let _img = new Image();
                _img.onload = function() {

                    let _sampleSize = 20;
                    _context.drawImage(_img, 0, 0, _width, _height);

                    let _data = _context.getImageData(0, 0, _sampleSize, _sampleSize);
                    let _length = _data.data.length;
                    let _counter = 0;
                    let _pixelJump = 1;
                    let _i = -4;
                    while ( (_i += _pixelJump * 4) < _length ) {
                        ++_counter;
                        _rgb.r += _data.data[_i];
                        _rgb.g += _data.data[_i+1];
                        _rgb.b += _data.data[_i+2];
                    }

                    _rgb.r = ~~(_rgb.r/_counter);
                    _rgb.g = ~~(_rgb.g/_counter);
                    _rgb.b = ~~(_rgb.b/_counter);

                    document.querySelector('.blog-post-hero-bg').style.background = _rgbToHex(_rgb);
                    document.querySelector('.blog-post-hero-bg-img').classList.add('fadeout');

                }
                _img.src = _imgUrl;
                _img.onerror = function() { console.log("Error loading image"); };

        };
        _xhr.send();
}


// ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~

document.addEventListener("DOMContentLoaded", function() {

    _toc();
    _tocOpenListener();
    _highlighter();
    _twitterButton();
    _sharerPosition();

    window.addEventListener('scroll', function() {

        _scrollBar();
        _checkActiveItem(_getSections());
    });
});

window.addEventListener("load", function() {

    _fiColor();
})
